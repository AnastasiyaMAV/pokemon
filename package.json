{
  "name": "pokemon",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "webpack": "webpack --config webpack.config.js ",
    "build:dev": "env NODE_ENV=development webpack --config webpack.config.js",
    "build:prod": "env NODE_ENV=production webpack --config webpack.config.js",
    "eslint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "pretty-quick": "pretty-quick"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "eslint": "7.12.1",
    "eslint-import-resolver-typescript": "^3.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.53.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.2.1",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "css-loader": "^6.7.1",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "4.3.0",
    "interpolate-html-plugin": "^4.0.0",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "pretty-quick --staged",
      "eslint ./src"
    ]
  }
}
